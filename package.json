  },
  "keywords": [
    "beautify",
    "format",
    "formatter",
    "html",
    "js",
    "js-beautify",
    "prettify",
    "pretty"
  ],
  "verb": {
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "lint": {
      "reflinks": true
    }
